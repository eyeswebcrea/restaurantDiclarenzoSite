        %}
        	<script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
{% endblock %}

{% block body %}
 <div class="span-22 prepend-1 prepend-top" id="nos-pizza">
 				{% for rubrique in boucleRubriques({"idRubrique" : 6}) %}
   					<h2 class="titre text-center">{{ rubrique.getTitre() }}</h2>
   				{% endfor %}
		
   		<div class="span-22 menu-list">
   		{% for pizza in boucleRubriques({"idParent" : 6}) %}
   			{% for menu in boucleArticles({"idRubrique" : pizza.getIdRubrique() }, { "titre" : "ASC" }) %}
   				<div class="block menu-item span-22 block {% if loop.index % 2 == 0 %}paire{% else %}impaire{% endif %}" id="menu-item-{{ loop.index}}">
   					<h2 class="block-title">{{ menu.getTitre() }}</h2>
   					<div class="block-content">
   						{{ menu.getTexte() | raw }}
   					</div>
   				</div>
   			{% endfor %}
   		{% endfor %}
   		</div>
		
