{% extends "::base.html.twig" %}


{% block stylesheets %}
		{{ parent() }}
	    {% 
        		stylesheets output='css/lacarte.css' 
        		'@SiteHomeBundle/Resources/public/css/lacarte.css'
        %}        		
        	<link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="screen" />
        {% endstylesheets %}
{% endblock %}

{% block javascripts %}	
		{{ parent() }}
        {%
        	javascripts output='js/lacarte.js'
